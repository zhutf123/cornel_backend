<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">


    <dubbo:application name="fh-product-server" owner="tao.zhang,ke.an,yongsheng.su,tengfei.zhu,qih.he,yisong.su,wxi.wang,zijie.zeng"/>

    <dubbo:registry
            address="zk.beta.corp.qunar.com:2181,zk1.beta.corp.qunar.com:2181,zk2.beta.corp.qunar.com:2181,zk3.beta.corp.qunar.com:2181"
            protocol="zookeeper"
            group="vc-fh-${env.profile.name}"
            timeout="60000"
            id="vc-fh" />

    <dubbo:registry
            id="ddr_server"
            protocol="zookeeper"
            address="zk.beta.corp.qunar.com:2181,zk1.beta.corp.qunar.com:2181,zk2.beta.corp.qunar.com:2181,zk3.beta.corp.qunar.com:2181"
            group="vc-local-${env.profile.name}"/>

    <dubbo:registry
            id="supplierServiceCenter"
            protocol="zookeeper"
            address="zk.beta.corp.qunar.com:2181,zk1.beta.corp.qunar.com:2181,zk2.beta.corp.qunar.com:2181,zk3.beta.corp.qunar.com:2181"
            group="/vs/tts/fengce/supplier"
            default="false"
            register="false"
            timeout="60000"
            check="true"/>

    <dubbo:consumer
            timeout="3000"
            retries="2"
            owner="tao.zhang,ke.an,yongsheng.su,tengfei.zhu,qih.he,yisong.su,wxi.wang,zijie.zeng"
    />

    <dubbo:registry id="flight-detail-api" register="false" protocol="zookeeper"
                    address="zk.beta.corp.qunar.com:2181,zk1.beta.corp.qunar.com:2181,zk2.beta.corp.qunar.com:2181,zk3.beta.corp.qunar.com:2181"
                    group="detail"/>

    <dubbo:registry protocol="zookeeper"
                    address="zk.beta.corp.qunar.com:2181,zk1.beta.corp.qunar.com:2181,zk2.beta.corp.qunar.com:2181,zk3.beta.corp.qunar.com:2181"
                    register="false"
                    id="qhotel-room-type-provider"
                    group="qhotel-room-type-provider-beta"/>
                    
    <dubbo:registry id="qhotel-inter-rt"
        protocol="zookeeper"
        address="zk.beta.corp.qunar.com:2181,zk1.beta.corp.qunar.com:2181,zk2.beta.corp.qunar.com:2181,zk3.beta.corp.qunar.com:2181"
        register="false"
        group="qhotel-inter-rt-beta"/>

    <dubbo:registry id="qhotel-image" protocol="zookeeper"
                    address="zk.beta.corp.qunar.com:2181,zk1.beta.corp.qunar.com:2181,zk2.beta.corp.qunar.com:2181,zk3.beta.corp.qunar.com:2181"
                    group="qhotel-image-beta"/>

    <dubbo:registry id="vc-tts"
                    protocol="zookeeper"
                    register="false"
                    address="zk.beta.corp.qunar.com:2181,zk1.beta.corp.qunar.com:2181,zk2.beta.corp.qunar.com:2181,zk3.beta.corp.qunar.com:2181"
                    group="vs/tts/recommend.bo"/>

    <import resource="dubbo-client-api.xml"/>

    <!-- 用dubbo协议在20880端口暴露服务 -->
    <dubbo:protocol name="dubbo" port="20900" register="true"/>
    <!--<dubbo:provider filter="legacyqmonitor"/>-->
</beans>
