<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

    <settings>
        <!-- 全局映射器启用缓存 -->
        <setting name="cacheEnabled" value="true"/>
        <!-- 查询时，关闭关联对象即时加载以提高性能 -->
        <setting name="lazyLoadingEnabled" value="false"/>
        <setting name="autoMappingBehavior" value="FULL"/>
    </settings>

    <typeAliases>
        <!--<typeAlias alias="ArrayStringHandler" type="com.qunar.vc.db.postgres.handler.ArrayStringSetTypeHandler"/>-->
        <!--<typeAlias alias="HotelDateStatusHandler" type="com.demai.cornel.util.HotelDateStatusTypeHandler"/>-->
        <!--<typeAlias alias="JsonTypeHandler" type="com.demai.cornel.util.JsonTypeHandler"/>-->
        <!--<typeAlias alias="JsonListTypeHandler" type="com.demai.cornel.util.JsonListTypeHandler"/>-->
        <!--<typeAlias alias="flightLine" type="com.qunar.vc.api.entity.flight.FlightLine"/>-->
        <!--<typeAlias alias="EnumHandler" type="com.qunar.vc.db.postgres.handler.IEmusHandler"/>-->
    </typeAliases>

    <typeHandlers>
        <typeHandler handler="ArrayStringHandler" javaType="java.util.Set"/>
        <typeHandler handler="JsonTypeHandler" javaType="java.util.Map"/>
    </typeHandlers>
</configuration>

